<%= form_for @teacher do |f| %>
  <%= f.label :name, 'Nome' %>
  <%= f.text_field :name %>
  <%= f.label :email, 'Email' %>
  <%= f.text_field :email %>
  <%= f.label :bio, 'Descrição' %>
  <%= f.text_field :bio %>
  <%= f.label :avatar, 'Foto' %>
  <%= f.file_field :avatar %>
  <%= f.submit 'Editar'%>
<% end %>

